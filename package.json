{
	"name": "your-management-front",
	"version": "1.0.2",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "bun --bun run dev",
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"deploy": "firebase deploy",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare-smui": "bun smui-theme-light && bun smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "3.2.2",
		"@sveltejs/kit": "2.5.18",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@types/eslint": "8.56.10",
		"@typescript-eslint/eslint-plugin": "7.15.0",
		"@typescript-eslint/parser": "7.15.0",
		"eslint": "9.6.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.1.3",
		"eslint-plugin-svelte": "2.41.0",
		"firebase-tools": "13.14.2",
		"prettier": "3.3.2",
		"prettier-plugin-svelte": "3.2.5",
		"svelte": "4.2.18",
		"svelte-adapter-bun": "0.5.2",
		"svelte-check": "3.8.4",
		"tslib": "2.6.3",
		"typescript": "5.5.3",
		"vite": "5.3.3",
		"vite-tsconfig-paths": "4.3.2"
	},
	"dependencies": {
		"@smui-extra/autocomplete": "7.0.0",
		"@smui/button": "7.0.0",
		"@smui/card": "7.0.0",
		"@smui/circular-progress": "7.0.0",
		"@smui/common": "7.0.0",
		"@smui/data-table": "7.0.0",
		"@smui/dialog": "7.0.0",
		"@smui/drawer": "7.0.0",
		"@smui/fab": "7.0.0",
		"@smui/form-field": "7.0.0",
		"@smui/icon-button": "7.0.0",
		"@smui/image-list": "7.0.0",
		"@smui/linear-progress": "7.0.0",
		"@smui/list": "7.0.0",
		"@smui/menu": "7.0.0",
		"@smui/menu-surface": "7.0.0",
		"@smui/paper": "7.0.0",
		"@smui/select": "7.0.0",
		"@smui/snackbar": "7.0.0",
		"@smui/switch": "7.0.0",
		"@smui/tab": "7.0.0",
		"@smui/tab-bar": "7.0.0",
		"@smui/textfield": "7.0.0",
		"@smui/tooltip": "7.0.0",
		"@smui/top-app-bar": "7.0.0",
		"@tomtom-international/web-sdk-maps": "6.25.0",
		"@tomtom-international/web-sdk-services": "6.25.0",
		"axios": "1.7.2",
		"cropperjs": "1.6.2",
		"smui-theme": "7.0.0",
		"svelte-material-ui": "7.0.0"
	}
}
