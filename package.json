{
  "name": "your-management-front",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "bun --bun run dev",
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "prepare-smui": "bun smui-theme-light && bun smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "3.1.1",
    "@sveltejs/kit": "2.5.0",
    "@sveltejs/vite-plugin-svelte": "3.0.2",
    "@types/eslint": "8.56.2",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-svelte": "2.35.1",
    "prettier": "3.2.4",
    "prettier-plugin-svelte": "3.1.2",
    "svelte": "4.2.9",
    "svelte-adapter-bun": "0.5.1",
    "svelte-check": "3.6.3",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-tsconfig-paths": "4.3.1"
  },
  "dependencies": {
    "@smui/button": "7.0.0-beta.16",
    "@smui/card": "7.0.0-beta.16",
    "@smui/common": "7.0.0-beta.16",
    "@smui/data-table": "7.0.0-beta.16",
    "@smui/drawer": "7.0.0-beta.16",
    "@smui/icon-button": "7.0.0-beta.16",
    "@smui/image-list": "7.0.0-beta.16",
    "@smui/linear-progress": "7.0.0-beta.16",
    "@smui/list": "7.0.0-beta.16",
    "@smui/paper": "7.0.0-beta.16",
    "@smui/tab": "7.0.0-beta.16",
    "@smui/tab-bar": "7.0.0-beta.16",
    "@smui/textfield": "7.0.0-beta.16",
    "@smui/top-app-bar": "7.0.0-beta.16",
    "@tomtom-international/web-sdk-maps": "6.25.0",
    "@tomtom-international/web-sdk-services": "6.25.0",
    "axios": "1.6.7",
    "smui-theme": "7.0.0-beta.16",
    "svelte-material-ui": "7.0.0-beta.16"
  }
}
