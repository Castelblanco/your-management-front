{
  "name": "your-management-front",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "bun --bun run dev",
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "prepare-smui": "bun smui-theme-light && bun smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "3.2.0",
    "@sveltejs/kit": "2.5.4",
    "@sveltejs/vite-plugin-svelte": "3.0.2",
    "@types/eslint": "8.56.6",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-svelte": "2.35.1",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.2",
    "svelte": "4.2.12",
    "svelte-adapter-bun": "0.5.2",
    "svelte-check": "3.6.8",
    "tslib": "2.6.2",
    "typescript": "5.4.3",
    "vite": "5.2.6",
    "vite-tsconfig-paths": "4.3.2"
  },
  "dependencies": {
    "@smui-extra/autocomplete": "7.0.0-beta.18",
    "@smui/button": "7.0.0-beta.18",
    "@smui/card": "7.0.0-beta.18",
    "@smui/circular-progress": "7.0.0-beta.18",
    "@smui/common": "7.0.0-beta.18",
    "@smui/data-table": "7.0.0-beta.18",
    "@smui/dialog": "7.0.0-beta.18",
    "@smui/drawer": "7.0.0-beta.18",
    "@smui/fab": "7.0.0-beta.18",
    "@smui/form-field": "7.0.0-beta.18",
    "@smui/icon-button": "7.0.0-beta.18",
    "@smui/image-list": "7.0.0-beta.18",
    "@smui/linear-progress": "7.0.0-beta.18",
    "@smui/list": "7.0.0-beta.18",
    "@smui/paper": "7.0.0-beta.18",
    "@smui/select": "7.0.0-beta.18",
    "@smui/snackbar": "7.0.0-beta.18",
    "@smui/tab": "7.0.0-beta.18",
    "@smui/tab-bar": "7.0.0-beta.18",
    "@smui/textfield": "7.0.0-beta.18",
    "@smui/tooltip": "7.0.0-beta.18",
    "@smui/top-app-bar": "7.0.0-beta.18",
    "@tomtom-international/web-sdk-maps": "6.25.0",
    "@tomtom-international/web-sdk-services": "6.25.0",
    "axios": "1.6.8",
    "smui-theme": "7.0.0-beta.18",
    "svelte-material-ui": "7.0.0-beta.18"
  }
}
